{
	"name": "transitions-kit",
	"version": "1.1.10",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/denchiklut/transitions-kit"
	},
	"homepage": "https://denchiklut.github.io/transitions-kit",
	"license": "MIT",
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"module.esnext": "dist/esnext.esm.js",
	"types": "dist/@types/index.d.ts",
	"files": [
		"dist"
	],
	"description": "A set of predefined transition components",
	"author": "Karl: ollylut@gmail.com",
	"scripts": {
		"build": "rm -rf dist && NODE_ENV=production && rollup -c",
		"start": "NODE_ENV=development webpack serve --mode development",
		"format": "prettier --write \"src/**/*.{ts,tsx}\"",
		"lint": "eslint --fix --quiet --ext .ts,.tsx ./"
	},
	"dependencies": {
		"react-transition-group": "^4.4.5"
	},
	"peerDependencies": {
		"react": ">=16.6.0",
		"react-dom": ">=16.6.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^22.0.1",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/react": "^18.0.15",
		"@types/react-transition-group": "^4.4.5",
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"eslint": "^8.23.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.4",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"rollup": "^2.77.2",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-terser": "^7.0.2",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"keywords": [
		"react-transition-group",
		"fade",
		"blur",
		"collapse",
		"react",
